{
  "name": "@gltf-transform/core",
  "version": "0.4.0-alpha.1",
  "repository": "github:donmccurdy/glTF-Transform",
  "description": "glTF 2.0 SDK for JavaScript, TypeScript, and Node.js",
  "main": "dist/core.umd.js",
  "module": "dist/core.modern.js",
  "source": "src/core.ts",
  "types": "dist/core.d.ts",
  "sideEffects": false,
  "scripts": {
    "dist": "microbundle --format modern,umd --define PACKAGE_VERSION=$npm_package_version",
    "watch": "microbundle watch --format modern,umd --define PACKAGE_VERSION=$npm_package_version",
    "watch:debug": "microbundle watch --no-compress --format modern,umd --define PACKAGE_VERSION=$npm_package_version"
  },
  "keywords": [
    "gltf",
    "3d",
    "model",
    "webgl",
    "threejs"
  ],
  "author": "Don McCurdy <dm@donmccurdy.com>",
  "license": "MIT",
  "files": [
    "dist/",
    "src/",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "dependencies": {
    "gl-matrix": "^3.3.0"
  },
  "mangle": {
    "regex": "^_"
  },
  "gitHead": "409abd1d5f6c00cd8a3252e8673a7bcad170bd11"
}
