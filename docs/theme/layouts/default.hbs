<script type="text/javascript" src="/assets/js/js.js"></script>

<script>
	(function() {
		const navItems = document.querySelectorAll('.tsd-navigation a');
		const locationHref = clean(location.href);
		navItems.forEach((item) => {
			if (clean(item.href) === locationHref) {
				item.classList.add('active');
			}
		});
		function clean (url) {
			return url.replace(/\.html.*/, '');
		}
	}())
</script>

